{
  "version": 2,
  "public": true,
  "installCommand": "npm install",
  "devCommand": "npm run test",
  "builds": [
    {
      "src": "/__tests__/**",
      "use": "@vercel/jest"
    },
    {
      "src": "./index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
